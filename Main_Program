#include <iostream>

using namespace std;

class stack {
	char data[MAX_STACK_SIZE];
	char top;
	
	public:
		stack() {
			top = -1;
		}
		
	
	void push(char input){
		if (top == MAX_STACK_SIZE-1) {
			cout << "Melebihi Batas Stack" << endl;
			return;
		}
		data[++top] = input;
	}
	
	void pop() {
		if (top == -1) {
			cout << "Stack Kosong" << endl;
			return;
		}
		top--;
	}
	
	int Top() {
		return data[top];
	}
};

int main () {
	
}
